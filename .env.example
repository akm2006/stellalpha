# StellAlpha Environment Variables
# Copy this file to .env.local and fill in your values

# Supabase (Required)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SECRET=sb_secret_your_secret_key
SUPABASE_PUBLISHABLE=sb_secret_your_publishable_key

# Helius API (Required for trade syncing)
HELIUS_API_KEY=your_helius_api_key
HELIUS_API_RPC_URL=https://mainnet.helius-rpc.com/?api-key=your_helius_api_key

# Jupiter API (Required for token metadata)
JUPITER_API_KEY=your_jupiter_api_key

# Helius Webhook Secret (for verifying webhook requests)
HELIUS_WEBHOOK_SECRET=your_random_secret_string

# Optional: Upstash Redis (for caching)
# UPSTASH_REDIS_REST_URL=
# UPSTASH_REDIS_REST_TOKEN=
