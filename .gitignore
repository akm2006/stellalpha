# ===========================================
# StellAlpha - Next.js Deployment .gitignore
# ===========================================

# Dependencies
node_modules/
.pnp/
.pnp.*
.yarn/*
!.yarn/patches
!.yarn/plugins
!.yarn/releases
!.yarn/versions

# Build outputs
.next/
out/
build/
dist/

# Environment files (secrets - never commit)
.env
.env.local
.env.production
.env.development
!.env.example

# IDE / Editor
.vscode/
.idea/
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Debug logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*

# TypeScript cache
*.tsbuildinfo
tsconfig.tsbuildinfo

# Vercel
.vercel

# Package locks (using pnpm)
package-lock.json

# Next.js generated
next-env.d.ts

# ===========================================
# Development / Non-deployment files
# ===========================================

# Anchor / Solana smart contracts (not part of frontend)
anchor/

# Observer scripts (development tools)
observer/

# Development scripts
scripts/

# Documentation (not needed for deployment)
docs/
helius-docs/
jupiter-fork-guide(surfpool)/
mainnet-fork-guide/

# Supabase migrations (not needed for Vercel)
supabase/

# Test ledger data
test-ledger/
test-ledger-debug/
test-ledger-fork/

# Misc
*.pem
coverage/
LICENSE
